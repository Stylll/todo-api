{
  "name": "todo-api",
  "version": "0.0.0",
  "private": true,
  "description": "App to manage a todo list",
  "main": "./server/bin/www",
  "scripts": {
    "migrate:db-prod": "sequelize db:migrate:undo:all --env=production && sequelize db:migrate --env=production",
    "migrate:db-dev": "sequelize db:migrate:undo:all --env=development && sequelize db:migrate --env=development",
    "migrate:db": "sequelize db:migrate:undo:all --env=test && sequelize db:migrate --env=test",
    "pretest": "npm run migrate:db",
    "test:server": "npm run pretest && set NODE_ENV=test&&mocha --compilers js:babel-core/register server/test/**/*.test.js --timeout 5000 --exit",
    "prestart": "babel-node tools/startMessage.js",
    "start": "babel-node tools/srcServer.js",
    "open:src": "npm-run-all --parallel open:app",
    "open:app": "babel-node tools/srcServer.js",
    "start:dev": "nodemon --exec babel-node server.js",
    "lint": "esw webpack.config.* tools client/src",
    "lint:watch": "npm run lint -- --watch",
    "prod:build": "npm run build",
    "clean-dist": "npm run remove-dist && mkdirp client/dist",
    "remove-dist": "rimraf client/dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm-run-all clean-dist build:html",
    "build": "babel-node tools/build.js",
    "postbuild": "babel-node tools/distServer.js",
    "open:build": "babel-node tools/distServer.js"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-core": "6.8.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.1",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-register": "6.8.0",
    "bcrypt": "^1.0.3",
    "body-parser": "~1.18.2",
    "cheerio": "0.22.0",
    "colors": "1.1.2",
    "compression": "1.6.1",
    "cookie-parser": "~1.4.3",
    "css-loader": "^0.28.11",
    "debug": "~2.6.9",
    "dotenv": "^5.0.1",
    "eventsource-polyfill": "0.9.6",
    "express": "~4.15.5",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "jsonwebtoken": "^8.2.0",
    "lodash": "^4.17.5",
    "materialize-css": "^0.100.2",
    "mini-css-extract-plugin": "^0.2.0",
    "mkdirp": "^0.5.1",
    "morgan": "~1.9.0",
    "nodemailer": "^4.6.2",
    "open": "0.0.5",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.6.1",
    "pug": "2.0.0-beta11",
    "randomstring": "^1.1.5",
    "react": "^16.2.0",
    "react-addons-test-utils": "15.0.2",
    "react-confirm-alert": "^2.0.2",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-redux-loading-bar": "^3.1.2",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "react-toastify": "^3.4.3",
    "redux": "^3.7.2",
    "redux-immutable-state-invariant": "1.2.3",
    "redux-mock-store": "1.0.2",
    "redux-thunk": "^2.2.0",
    "sequelize": "^4.33.4",
    "serve-favicon": "~2.4.5",
    "style-loader": "^0.20.3",
    "toastr": "^2.1.4",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "url-loader": "^1.0.1",
    "validator": "^9.4.1",
    "webpack": "^4.1.1",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.10.0"
  },
  "keywords": [
    "Todo, List"
  ],
  "author": "Stephen Aribaba",
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "cross-env": "1.0.7",
    "enzyme": "2.2.0",
    "eslint": "^4.18.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "eslint-watch": "2.1.11",
    "expect": "1.19.0",
    "jsdom": "8.5.0",
    "mocha": "^5.0.1",
    "nock": "8.0.0",
    "npm-run-all": "1.8.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.20.3",
    "supertest": "^3.0.0"
  }
}
